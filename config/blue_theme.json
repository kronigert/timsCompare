    "CTkScrollbar": {
        "corner_radius": 1000,
        "border_spacing": 4,
        "fg_color": "transparent",
        "button_color": "#C7D2D9",
        "button_hover_color": "#83928C"
    },
	"CTkEntry": {
		"corner_radius": 6,
		"border_width": 1,
		"fg_color": "#E4EFF7",
		"border_color": "#0071BC",
		"text_color": "#04304D",
		"placeholder_text_color": "#6D7A84"
	},
	"CTkSegmentedButton": {
		"corner_radius": 6,
		"border_width": 2,
		"fg_color": "#004E82",
		"selected_color": "#0071BC",
		"selected_hover_color": "#0071BC",
		"unselected_color": "#004E82",
		"unselected_hover_color": "#005d9e",
		"text_color": "#E4EFF7",
		"text_color_disabled": "#C7D2D9"
	}
